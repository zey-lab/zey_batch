# ============================================
# Twilio Credentials (REQUIRED for live mode)
# ============================================
TWILIO_ACCOUNT_SID=your_account_sid_here
TWILIO_AUTH_TOKEN=your_auth_token_here
TWILIO_PHONE_NUMBER=+1234567890

# ============================================
# Logging Configuration
# ============================================
LOG_LEVEL=INFO

# ============================================
# OPTIONAL OVERRIDES
# ============================================
# The settings below can override config.yml settings if needed.
# Leave commented to use config.yml values.

# Override dry run mode
# DRY_RUN=false

# Override test phone numbers (comma-separated)
# TEST_PHONE_NUMBERS=+14155551234,+14155551235

# Override phone number column name
# PHONE_NUMBER_COLUMN=phone_number

# Override file names
# CUSTOMERS_LIST_OLD=customers_list_old.xlsx
# CUSTOMERS_LIST_NEW=customers_list_new.xlsx
# CAMPAIGN_CONFIG=campaigns.xlsx
